{
    "name": "phpbb/phpbb-website",
    "description": "The phpBB website located at www.phpbb.com",
    "version": "4.0.0-dev",
    "keywords": ["website", "phpbb"],
    "homepage": "http://www.phpbb.com",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "phpBB Group",
            "email": "website@phpbb.com",
            "homepage": "http://phpbb.com/"
        }
    ],
    "support": {
        "irc": "irc://irc.freenode.org/phpbb-dev",
        "issues": "https://www.phpbb.com/bugs/website/"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "doctrine/orm": "~2.2",
        "doctrine/doctrine-bundle": "1.3.*@beta",
        "doctrine/doctrine-migrations-bundle": "dev-master@dev",
        "doctrine/migrations": "dev-master@dev",
        "symfony/icu": "1.0.*",
        "php": ">=5.3.3",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "symfony/assetic-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/symfony": "2.4.*",
        "twig/extensions": "1.0.*",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/data-fixtures": "dev-master",
        "friendsofsymfony/user-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
